{
  "manifest_version": 3,
  "name": "Fable",
  "author": "fable",
  "version": "1.0.0",
  "version_name": "1.0.0",
  "description": "",
  "icons": {},
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "activeTab", "storage", "debugger", "unlimitedStorage"],
  "optional_host_permissions": ["*://*/*"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "host_permissions": ["http://localhost:8080/api/*"]
}
