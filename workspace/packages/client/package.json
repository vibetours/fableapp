{
  "name": "@fable/client",
  "version": "1.0.0",
  "description": "Fable app UI",
  "repository": "git+ssh://git@github.com:sharefable/app.git",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "antd": "^5.1.2",
    "react-icons": "^4.6.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.56",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "env-cmd": "^10.1.0",
    "less": "^4.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "serve": "^14.0.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.8.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "env-cmd --verbose -e dev -r ../env.json react-scripts start",
    "start-stage": "env-cmd --verbose -e stage -r ../env.json react-scripts start",
    "build-staging": "env-cmd --verbose -e stage -r ../env.json react-scripts build",
    "install-staging": "aws s3 cp ./build/ s3://app.sharefable.com --recursive --profile fable",
    "build": "env-cmd -e prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "brand-asset-gen": "lessc ./style_overrides/brand_overrides.less ./src/brand.css --js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
