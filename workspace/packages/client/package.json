  "scripts": {
    "start": "env-cmd --verbose -e dev -r ../env.json react-scripts start",
    "start-stage": "env-cmd --verbose -e stage -r ../env.json  react-scripts start",
    "build-staging": "env-cmd --verbose -e stage -r ../env.json react-scripts build",
    "clean-staging": "aws s3 rm s3://app.sharefable.com --recursive --profile fable",
    "install-staging": "yarn clean-staging && aws s3 cp ./build/ s3://app.sharefable.com --recursive --profile fable",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
