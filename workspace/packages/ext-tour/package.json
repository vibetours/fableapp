{
  "name": "@fable/ext-tour",
  "version": "1.6.7",
  "main": "build",
  "description": "Product demo extension for Fable",
  "repository": "git+ssh://git@github.com:sharefable/app.git",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "prebuild-local": "yarn clean",
    "build-local": "env-cmd --verbose -e local -r ../env.json webpack",
    "prebuild-dev": "yarn clean",
    "build-dev": "env-cmd --verbose -e dev -r ../env.json webpack",
    "test-debug": "node --inspect-brk ./node_modules/.bin/jest",
    "test": "jest --verbose",
    "prebuild-staging": "yarn clean",
    "build-staging": "env-cmd --verbose -e staging -r ../env.json webpack",
    "build-prod": "env-cmd --verbose -e prod -r ../env.json webpack",
    "dupdetect": "jscpd src/"
  },
  "dependencies": {
    "@fable/common": "^1.0.0",
    "@headlessui/react": "^0.0.0-dev.0ce63d8",
    "@sentry/browser": "^7.49.0",
    "nanoid": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@sentry/types": "^7.49.0",
    "@types/chrome": "^0.0.200",
    "@types/jest": "^25.2.1",
    "@types/jsdom": "^20.0.1",
    "@types/nanoid": "^3.0.0",
    "@types/node": "12.12.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/sha1": "^1.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^3.5.3",
    "env-cmd": "^10.1.0",
    "envsub": "^4.0.7",
    "eslint": "8.31.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^25.5.4",
    "jscpd": "^3.5.9",
    "jsdom": "^20.0.2",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^1.2.0",
    "ts-jest": "^25.4.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  }
}
